{
  "grammars": [
    {
      "name": "plantuml",
      "camelcase": "PlantUmlSitter",
      "scope": "source.plantuml",
      "path": ".",
      "file-types": [
        "plantuml",
        "puml"
      ],
      "injection-regex": "^plantuml$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A sitter for Plantuml Files",
    "authors": [
      {
        "name": "David Schlueter"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-plantuml_sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
